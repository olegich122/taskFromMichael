
FROM node:carbon

#  App directory
WORKDIR /usr/src/app

# Install app dependencies
# A wildcard is used to ensure both package.json AND package-lock.json are copied
# where available (npm@5+)
COPY package.json .

# Bundle app source
#COPY . .

RUN npm install 
# If you are building your code for production
# RUN npm install --only=production
COPY . .

EXPOSE 13062
#CMD ["/bin/bash"]
CMD [ "npm", "start" ]
